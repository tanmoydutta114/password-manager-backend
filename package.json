{
  "name": "cloud-run",
  "version": "2.9.0",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "prettier-all": "prettier --write src/**/*.{ts,tsx,js,css,md} --ignore-path .gitignore",
    "start-link-ts": "yarn linking && yarn build && yarn serve",
    "start-ts": "yarn build && yarn serve",
    "start": "yarn serve",
    "start-dev": "nodemon",
    "build": "tsc",
    "serve": "node dist/index.js",
    "test": "jest --setupFiles dotenv/config",
    "watch-test": "yarn test -- --watchAll",
    "watch-ts": "tsc -w",
    "link-types": "yarn link @witbybit/decorum-types",
    "unlink-types": "yarn unlink @witbybit/decorum-types && yarn install --force",
    "update-prisma": "npx prisma db pull && npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.2.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "express": "^4.19.2",
    "immer": "9.0.15",
    "jsonwebtoken": "^9.0.0",
    "lodash": "4.17.21",
    "multer": "^1.4.2",
    "papaparse": "^5.2.0",
    "zod": "^3.10.3",
    "dompurify": "^3.0.9",
    "jsdom": "^22.1.0",
    "nanoid": "3",
    "kysely": "^0.27.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "4.17.6",
    "@types/jest": "^28.1.7",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.155",
    "@types/multer": "1.4.3",
    "@types/node": "14.0.12",
    "@types/papaparse": "^5.3.2",
    "@types/uuid": "^8.3.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.1",
    "husky": "^6.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.15",
    "prettier": "2.4.1",
    "prisma": "5.2.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslint": "6.1.3",
    "typescript": "5.4.5",
    "@types/dompurify": "^3.0.4",
    "@types/jsdom": "^21.1.4",
    "prisma-kysely": "^1.8.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write"
  }
}
